{
  "expo": {
    "name": "TradeUp",
    "slug": "TradeUp",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/tradeup.png",
    "userInterfaceStyle": "light",
    "notification": {
      "icon": "./assets/tradeup.png",
      "color": "#311b92",
      "androidMode": "default",
      "androidCollapsedTitle": "Updates from Colab.notes",
      "iosDisplayInForeground": true
    },
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.alextrade432.TradeUp"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/tradeup.png",
        "backgroundColor": "#FFFFFF"
      }
    },
    "web": {
      "favicon": "./assets/tradeup.png"
    },
    "plugins": [
      [
        "expo-notifications",
        {
          "icon": "./assets/tradeup.png",
          "color": "#ffffff"
        }
      ]
    ]
  }
}
